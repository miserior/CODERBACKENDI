<div class="row sectionc">
  <div>
    <h2>Productos destacados</h2>
    <p>Experiencia y compromiso en cada servicio.</p>
  </div>
  <section class="row justify-content-center">
    {{#each products}}

      <div class="card sectionc-cards col-lg-3 col-md-2 col-sm-12">
        <div class="card-body">
          <h3 class="card-title">{{this.title}}</h3>
          <h4 class="card-title">{{this.category}}</h4>
          <p class="card-text">{{this.price}} USD</p>
        </div>
      </div>
    {{/each}}

  </section>
  <div>
    {{#each links}}
      <a href="{{this.link}}" class="btn btn-primary">{{this.text}}</a>
    {{/each}}
  </div>
</div>